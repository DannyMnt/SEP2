<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.RegisterUserViewController">
   <children>
      <VBox alignment="CENTER" prefHeight="401.0" prefWidth="600.0">
         <children>
            <Label text="Register">
               <padding>
                  <Insets bottom="20.0" />
               </padding>
               <font>
                  <Font name="System Bold" size="16.0" />
               </font>
            </Label>
            <HBox alignment="CENTER" prefHeight="35.0" prefWidth="600.0">
               <children>
                  <VBox fx:id="vbox1" alignment="CENTER" prefHeight="100.0" prefWidth="130.0">
                     <children>
                        <Label text="Email">
                           <VBox.margin>
                              <Insets />
                           </VBox.margin></Label>
                     </children>
                  </VBox>
                  <VBox fx:id="vbox2" alignment="CENTER" prefHeight="35.0" prefWidth="160.0">
                     <children>
                        <TextField fx:id="emailTextField" prefHeight="25.0" prefWidth="151.0" />
                     </children>
                  </VBox>
               </children>
            </HBox>
            <Label fx:id="errorLabel" textFill="RED" />
            <Button fx:id="continueBtn" mnemonicParsing="false" onAction="#continueBtn" text="Continue">
               <VBox.margin>
                  <Insets top="10.0" />
               </VBox.margin>
            </Button>
         </children>
      </VBox>
   </children>
</AnchorPane>
